{
  "manifest_version": 3,
  "name": "Miro Redirector",
  "version": "1.0",
  "description": "Redirects Miro URLs to the miroapp:// protocol.",
  "permissions": [
    "webNavigation",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "https://miro.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
